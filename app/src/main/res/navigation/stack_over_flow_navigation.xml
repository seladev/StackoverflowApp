<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/stack_over_flow_navigation"
    app:startDestination="@id/feed_fragment">

    <fragment
        android:id="@+id/feed_fragment"
        android:name="com.sela.stackoverflowapp.ui.feed.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed" >

        <action
            android:id="@+id/action_feed_fragment_to_question_fragment"
            app:destination="@id/question_fragment"/>

    </fragment>

    <fragment
        android:id="@+id/question_fragment"
        android:name="com.sela.stackoverflowapp.ui.question.QuestionFragment"
        android:label="fragment_question"
        tools:layout="@layout/fragment_question" />

</navigation>